<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dragonos.tracker.mapper.ContributorMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.dragonos.tracker.pojo.Contributor">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="pr_num" property="prNum" />
        <result column="issue_num" property="issueNum" />
        <result column="contributions" property="contributions" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, pr_num, issue_num, contributions
    </sql>

</mapper>
